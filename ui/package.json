{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build_template": "yarn run clean && yarn build && yarn run copy",
    "clean": "rm -rf dist && rm -rf ../api/templates/v2_ui",
    "copy": "cp -a dist/ ../api/templates/v2_ui/",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "NODE_ENV=test jest",
    "coverage": "NODE_ENV=test jest --collectCoverage",
    "lint": "./node_modules/eslint/bin/eslint.js . --ext .jsx --ext .jsx,.js,.ts,.tsx",
    "lint-fix": "./node_modules/eslint/bin/eslint.js . --fix --ext .jsx,.js,.ts,.tsx",
    "prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}'",
    "prettier:fix": "prettier --loglevel warn --write 'src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}'",
    "format": "npm run prettier:fix && npm run lint:fix",
    "cypress:open": "TZ=UTC cypress open",
    "cypress:run": "TZ=UTC cypress run"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@cloudscape-design/global-styles": "^1.0.6",
    "@hookform/resolvers": "^2.9.10",
    "@monaco-editor/loader": "^1.3.3",
    "@monaco-editor/react": "^4.4.6",
    "@noqdev/cloudscape": "^1.0.0",
    "@sentry/react": "^7.1.1",
    "@sentry/tracing": "^7.1.1",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-table": "^8.9.1",
    "apollo-link-rest": "^0.9.0-rc.2",
    "axios": "^1.2.1",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "framer-motion": "^7.6.9",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "luxon": "^3.3.0",
    "monaco-editor": "^0.37.1",
    "monaco-yaml": "^4.0.4",
    "nanoid": "^4.0.0",
    "prismjs": "^1.29.0",
    "reablocks": "^3.6.20",
    "react": "^18.2.0",
    "react-auth-code-input": "^3.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.36.1",
    "react-joyride": "^2.5.4",
    "react-markdown": "^8.0.7",
    "react-qr-code": "^2.0.8",
    "react-router-dom": "^6.4.2",
    "react-toastify": "^9.1.2",
    "react-use": "^17.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@faker-js/faker": "^8.0.1",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-docs": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/builder-vite": "^0.2.3",
    "@storybook/react": "6.5.12",
    "@swc/core": "^1.3.51",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "crypto-js": "^4.1.1",
    "cypress": "^12.11.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^9.1.9",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^13.2.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.2.1",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}": [
      "prettier --write "
    ]
  },
  "resolutions": {
    "**/json5": "^2.2.2",
    "**/trim-newlines": "^3.0.1",
    "**/glob-parent": "^5.1.2",
    "**/trim": "^0.0.3",
    "**/webpack": "^5.76.0"
  }
