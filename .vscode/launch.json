{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Local Config: CloudUmi Celery",
      "args": [
        "-A",
        "common.celery_tasks.celery_tasks",
        "worker",
        "-l",
        "DEBUG",
        "-B",
        "--concurrency",
        "8"
      ],
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "celery",
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/configs/development_account/saas_development.yaml",
        // "CONFIG_LOCATION": "s3://noq-tenant-configuration.node.dev1.259868150464.us-west-2/tenant_configurations/saas_development.yaml",
        "AWS_PROFILE": "noq_cluster_dev",
        "PYTHONPATH": "$PYTHONPATH:${workspaceFolder}"
      }
    },
    {
      "name": "Local Config: Initialize DynamoDB",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/common/scripts/initialize_dynamodb.py",
      "console": "integratedTerminal",
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/configs/development_account/saas_development.yaml",
        // "CONFIG_LOCATION": "s3://noq-tenant-configuration.node.dev1.259868150464.us-west-2/tenant_configurations/saas_development.yaml",
        "AWS_PROFILE": "noq_cluster_dev",
        "AWS_DEFALT_REGION": "us-west-2",
        "PYTHONPATH": "$PYTHONPATH:${workspaceFolder}"
      }
    },
    {
      "name": "Local Config: CloudUmi API",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/api/__main__.py",
      "console": "integratedTerminal",
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/configs/development_account/saas_development.yaml",
        // "CONFIG_LOCATION": "s3://noq-tenant-configuration.node.dev1.259868150464.us-west-2/tenant_configurations/saas_development.yaml",
        "AWS_PROFILE": "noq_cluster_dev",
        "PYTHONPATH": "$PYTHONPATH:${workspaceFolder}"
      }
    },
    {
      "name": "Local Config: Initialize Redis",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/common/scripts/initialize_redis.py",
      "console": "integratedTerminal",
      "env": {
        // "CONFIG_LOCATION": "${workspaceFolder}/cloudumi_config/configs/development_account/saas_development.yaml",
        "CONFIG_LOCATION": "${workspaceFolder}/configs/development_account/saas_development.yaml",
        "AWS_PROFILE": "noq_cluster_dev",
        "PYTHONPATH": "$PYTHONPATH:${workspaceFolder}"
      }
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "https://localhost",
      "webRoot": "${workspaceFolder}/frontend/",
      "env": {
        "INTERPRETER_OPTIONS": "",
        "PARENT_ENVS": true,
        "PYTHONUNBUFFERED": "1",
        "PYTHONASYNCIODEBUG": "1"
      }
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/configs/development_account/saas_development.yaml",
        "AWS_PROFILE": "noq_dev",
        "PYTHONPATH": "${workspaceFolder}/"
      }
    },
    {
      "name": "Debug test",
      "type": "python",
      "request": "launch",
      "console": "externalTerminal",
      "justMyCode": false,
      "stopOnEntry": true,
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/util/tests/test_configuration.yaml",
        "PYTHONPATH": "${workspaceFolder}/"
      }
    }
  ]
}
