        "AWS_PROFILE": "development/NoqSaasRoleLocalDev",
        "PYTHONPATH": "$PYTHONPATH:${workspaceFolder}",
        "PYTHONASYNCIODEBUG": "1",
        "RUNTIME_PROFILE": "CELERY_FLOWER"
