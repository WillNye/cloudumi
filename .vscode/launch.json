{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Celery",
      "args": [
        "-A",
        "cloudumi_common.celery_tasks.celery_tasks",
        "worker",
        "-l",
        "DEBUG",
        "-P",
        "solo",
        "--concurrency",
        "8"
      ],
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "celery",
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/cloudumi_config/configs/development_account/saas_development.yaml",
        "AWS_PROFILE": "personal"
      }
    },
    {
      "name": "Python: CloudUmi API",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/cloudumi_api/cloudumi_api/__main__.py",
      "console": "integratedTerminal",
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/cloudumi_config/configs/development_account/saas_development.yaml",
        "AWS_PROFILE": "personal"
      }
    },
    {
      "name": "Python: Initialize Dynamo Tables",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/cloudumi_common/cloudumi_common/scripts/initialize_dynamodb.py",
      "console": "integratedTerminal",
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/cloudumi_config/configs/development_account/saas_development.yaml",
        "AWS_PROFILE": "personal"
      }
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost",
      "webRoot": "${workspaceFolder}/cloudumi_frontend/frontend/",
      "env": {
        "INTERPRETER_OPTIONS": "",
        "PARENT_ENVS": true,
        "PYTHONUNBUFFERED": "1",
        "PYTHONASYNCIODEBUG": "1",
        "CONSOLEME_CONFIG_ENTRYPOINT": "cmsaas_config",
        "CONFIG_LOCATION": "${workspaceFolder}/cloudumi_config/configs/localhost/saas_development.yaml",
        "AWS_PROFILE": "localstack"
      }
    },
    {
      "name": "Remote: cloudumi-healthcheck",
      "type": "python",
      "request": "attach",
      "host": "localhost",
      "port": 9090,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/cloudumi_healthcheck",
          "remoteRoot": "/apps/cloudumi_healthcheck"
        }
      ]
    },
    {
      "name": "Remote: cloudumi-saml",
      "type": "python",
      "request": "attach",
      "host": "localhost",
      "port": 9091,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/cloudumi_saml",
          "remoteRoot": "/apps/cloudumi_saml"
        },
        {
          "localRoot": "${workspaceFolder}/cloudumi_common",
          "remoteRoot": "/apps/cloudumi_common"
        }
      ]
    },
    {
      "name": "Remote: cloudumi-api",
      "type": "python",
      "request": "attach",
      "host": "localhost",
      "port": 9092,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/cloudumi_api",
          "remoteRoot": "/apps/cloudumi_api"
        },
        {
          "localRoot": "${workspaceFolder}/cloudumi_common",
          "remoteRoot": "/apps/cloudumi_common"
        },
        {
          "localRoot": "${workspaceFolder}/cloudumi_saml",
          "remoteRoot": "/apps/cloudumi_saml"
        }
      ]
    },
    {
      "name": "Local: cloudumi-api",
      "type": "python",
      "request": "attach",
      "host": "localhost",
      "port": 9092,
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/cloudumi_config/configs/development_account/saas_development.yaml",
        "AWS_PROFILE": "personal"
      }
    },
    {
      "name": "Remote: cloudumi_initialize_redis_dynamo_for_dev",
      "type": "python",
      "request": "attach",
      "host": "localhost",
      "port": 9099,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/cloudumi_api",
          "remoteRoot": "/apps/cloudumi_api"
        },
        {
          "localRoot": "${workspaceFolder}/cloudumi_common",
          "remoteRoot": "/apps/cloudumi_common"
        }
      ]
    }
  ]
}
