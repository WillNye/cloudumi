        "AWS_REGION": "us-west-2",
        "AWS_PROFILE": "staging/staging_admin",
        "PYTHONPATH": "$PYTHONPATH:${workspaceFolder}",
        "STAGE": "staging",
        "TEST_USER_DOMAIN": "localhost",
        "VERSION": "1.5.571"
      }
    },
    {
      "name": "Debug test",
      "type": "python",
      "request": "launch",
      "console": "externalTerminal",
      "justMyCode": false,
      "stopOnEntry": false,
      "env": {
        "CONFIG_LOCATION": "${workspaceFolder}/util/pytest/test_configuration.yaml",
