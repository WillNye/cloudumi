        "AWS_CONTAINER_CREDENTIALS_FULL_URI": "http://localhost:9091/ecs/arn:aws:iam::759357822767:role/NoqSaasRoleLocalDev",
        //"AWS_PROFILE": "development/NoqSaasRoleLocalDev_2",
        "PYTHONPATH": "$PYTHONPATH:${workspaceFolder}",
